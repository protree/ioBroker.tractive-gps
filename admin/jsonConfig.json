{
    "i18n": true,
    "type": "tabs",
    "items": {
        "_login": {
            "label": "settings_login",
            "type": "panel",
            "items": {
                "_header1": {
                    "newLine": true,
                    "type": "header",
                    "size": 4,
                    "text": "settings_interval",
                    "sm": 12
                },
                "interval": {
                    "type": "number",
                    "label": "settings_interval",
                    "min": 1,
                    "help": "settings_sec",
                    "tooltip": "settings_interval_tooltip",
                    "sm": 12,
                    "md": 6,
                    "lg": 4
                },
                "_header2": {
                    "newLine": true,
                    "type": "header",
                    "text": "settings_login",
                    "size": 4,
                    "sm": 12
                },
                "email": {
                    "newLine": true,
                    "label": "emailInput_email",
                    "type": "text",
                    "sm": 12,
                    "md": 6,
                    "lg": 4
                },
                "password": {
                    "type": "password",
                    "label": "settings_password",
                    "repeat": true,
                    "sm": 12,
                    "md": 12,
                    "lg": 8
                },
                "_header3": {
                    "newLine": true,
                    "type": "header",
                    "text": "settings_token",
                    "size": 4,
                    "sm": 12
                },
                "user_id": {
                    "newLine": true,
                    "label": "settings_user_id",
                    "type": "text",
                    "readOnly": true,
                    "sm": 12,
                    "md": 6,
                    "lg": 4
                },
                "access_token": {
                    "newLine": true,
                    "label": "apiToken_accessToken",
                    "type": "password",
                    "repeat": "false",
                    "visible": true,
                    "readOnly": true,
                    "sm": 12,
                    "md": 6,
                    "lg": 4
                },
                "expires_at": {
                    "label": "dateTime_token_expires_tooltip",
                    "type": "text",
                    "time": true,
                    "sm": 12,
                    "md": 6,
                    "lg": 4
                },
                "_refreshToken": {
                    "type": "sendto",
                    "disabled": "!data.email || !data.password",
                    "variant": "contained",
                    "label": "apiToken_refresh",
                    "icon": "refresh",
                    "command": "refreshToken",
                    "jsonData": "{\"email\": \"${data.email}\", \"password\": \"${data.password}\"}",
                    "useNative": true,
                    "title": "apiToken_refresh_description",
                    "sm": 12,
                    "md": 6,
                    "lg": 4
                }
            }
        },
        "_devices": {
            "label": "settings_devices",
            "type": "panel",
            "items": {
                "nameArray": {
                    "type": "table",
                    "uniqueColumns": ["id"],
                    "sm": 12,
                    "items": [
                        {
                            "attr": "id",
                            "title": "deviceID",
                            "type": "text",
                            "readOnly": true,
                            "width": "50%"
                        },
                        {
                            "attr": "name",
                            "title": "deviceName",
                            "type": "text",
                            "width": "50%"
                        }
                    ]
                }
            }
        }
    }
}
